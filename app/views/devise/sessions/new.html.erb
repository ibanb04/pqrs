 <div class="row"> 
    <div class="card hoverable" style="background-color: rgba(255,255,255,.15);" id="card-login">
        
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="card-content grey lighten-4">
      <span class="card-title center"><h2>Login</h2></span>
        <div class="row">
          <div class="field col s12 l12">
            <%= f.label :email %><br />
            <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
          </div>
          <div class="field col s12 l12">
            <%= f.label :password %><br />
            <%= f.password_field :password, autocomplete: "off" %>
          </div>
        </div>
      <div class="row">
          <div class="center-align">
            <%= render "devise/shared/links" %>
          </div>
      </div>
      </div>
        <div class="actions  card-action white center-align">
          <%= f.submit "Login",class:'btn btn-large waves-effect waves-light blue darken-3 hoverable pulse ',:style => "color: white !important; " %>
        </div>
      <% end %>

    </div>
</div>
